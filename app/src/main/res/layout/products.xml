        android:text="Tilføj til indkøbslisten"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="376dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/addProduct"
        android:layout_width="356dp"
